  "license": "MIT",
  "homepage": "https://agilevibecoding.org",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NachoColl/agilevibecoding.git",
    "directory": "src"
  },
  "bugs": {
    "url": "https://github.com/NachoColl/agilevibecoding/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.20.0",
    "@google/genai": "^1.37.0",
    "chokidar": "^4.0.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.0",
    "express": "^4.21.2",
    "ink": "^5.0.1",
    "ink-select-input": "^6.0.0",
    "ink-spinner": "^5.0.0",
    "marked": "^15.0.6",
    "openai": "^6.19.0",
    "react": "^18.3.1",
    "vitepress": "^1.6.4",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@sinonjs/fake-timers": "^12.0.0",
    "@vitest/coverage-v8": "^2.1.9",
    "@vitest/ui": "^2.1.9",
    "memfs": "^4.56.10",
    "sinon": "^19.0.5",
    "vitest": "^2.1.9"
  }
